{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Redundancy Rate",
      "anchor": "start",
      "subtitle": "Per 1000 employees | Source: ONS through ECO API",
      "fontSize": 18,
      "subtitleFontSize": "14",
      "color": "#000000",
      "subtitleColor": "#000000"
    },
    "height": 300,
    "width": 500,
    "autosize": {
      "type": "fit",
      "contains": "padding"
    },
    "background": "#ffffff",
    "view": {
      "fill": "#ffffff",
      "strokeOpacity": 0
    },
    "data": {
      "url": "https://api.economicsobservatory.com/gbr/redn?vega",
      "format": {
        "type": "json"
      }
    },
    "mark": {
      "type": "line",
      "color": "#f4134d",
      "strokeWidth": "2"
    },
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      }
    }
  }